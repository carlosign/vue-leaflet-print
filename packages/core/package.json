{
  "name": "@carlosign/vue-leaflet-print",
  "version": "0.2.0",
  "description": "Control de impresión/exportación para Vue-Leaflet (Vue 3): print nativo, PNG via html-to-image y PNG via leaflet-image.",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": ["dist", "README.md"],
  "sideEffects": ["./dist/style.css","./src/styles.css"],
  "repository": {
    "type": "git",
    "url": "https://github.com/carlosign/vue-leaflet-print.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/carlosign/vue-leaflet-print#readme",
  "bugs": {
    "url": "https://github.com/carlosign/vue-leaflet-print/issues"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "leaflet": "^1.9.0",
    "@vue-leaflet/vue-leaflet": "^0.10.0"
  },
  "dependencies": {
    "html-to-image": "^1.11.11",
    "leaflet-image": "^0.2.0"
  },
  "devDependencies": {
    "vite": "^5.4.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "vite-plugin-dts": "^4.2.3",
    "typescript": "^5.5.0",
    "vue": "^3.4.0",
    "eslint": "^9.0.0",
    "@types/leaflet": "^1.9.8",
    "vitest": "^2.0.0",
    "@vue/test-utils": "^2.4.6",
    "semantic-release": "^23.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/github": "^10.0.7",
    "conventional-changelog-conventionalcommits": "^8.0.0"
  },
  "scripts": {
    "build": "vite build -c vite.config.ts",
    "test": "vitest run",
    "release": "semantic-release"
  },
  "keywords": ["leaflet","vue","vue-leaflet","print","export","map","png"],
  "author": "Carlos Ignacio Charletti",
  "license": "MIT"
}
